{
  "tags": {
    "allowUnknownTags": true,
    "dictionaries": [
      "jsdoc"
    ]
  },
  "recurseDepth": 10,
  "source": {
    "include": [
      "./module"
    ],
    "includePattern": ".+\\.(js)$",
    "excludePattern": "(^|\\/|\\\\)_"
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": true,
    "useLongnameInNav": false,
    "showInheritedInNav": true
  },
  "opts": {
    "destination": "./out",
    "encoding": "utf8",
    "recurse": true
  }
}